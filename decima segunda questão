#include <stdio.h>
#include <stdlib.h>  

int main() {
    int X, Y;

    printf("Digite X para linhas e Y para ate onde vai: ");
    scanf("%d %d", &X, &Y);

    for (int i = 1; i <= Y; i++) {
        printf("%d", i); 

        if (i % X != 0 && i != Y) {
            printf(" ");
        }

        if (i % X == 0) {
            printf("\n");
        }
    }

    system("pause");

    return 0;
}
