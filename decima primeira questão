#include <stdio.h>
#include <stdlib.h> 

int main() {
    int N, a = 0, b = 1, c;

    printf("Bem vindo a aplicação da série Fibonacci!\n");
    printf("Insira o valor que você quer descobrir:");
    scanf("%d", &N);
    printf("O resultado da aplicação de Fibonacci é:\n");
    
    if (N > 0) {
        printf("%d", a); 
    }
    
   
    if (N > 1) {
        printf(" %d", b); 
    }

    
    for (int i = 2; i < N; i++) {
        c = a + b;  
        printf(" %d", c);  
        a = b;  
        b = c;  
    }

    system("pause");

    return 0;
}
